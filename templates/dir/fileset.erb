
<% if defined?(@description) -%>
#
# <%= @description %>
#
<% end -%>
FileSet {
  Name = "<%= @fileset_name %>"
  Include {
    Options {
      <%- if @gzip -%>
      compression=GZIP<%= @gzip_level %>
      <%- end -%>
      signature = <%= @signature %>
      onefs = <%= scope.function_bool2yesno([@onefs]) %>
    }
    <%- @includelist.each do |value| -%>
    File = <%= value %>
    <%- end -%>
  }

  Exclude {
    <%- @excludelist.each do |value| -%>
    File = <%= value %>
    <%- end -%>
  }
}
